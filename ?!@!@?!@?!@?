-- Scripts:

local function QRYWNJ_fake_script() -- InfiniteYield.LocalScript 
	local script = Instance.new('LocalScript', InfiniteYield)

	InfiniteYield.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
	end)
end
coroutine.wrap(QRYWNJ_fake_script)()
local function HIGFUKZ_fake_script() -- DarkDex.LocalScript 
	local script = Instance.new('LocalScript', DarkDex)

	DarkDex.MouseButton1Click:Connect(function()
		loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/Shadow-Developer/Shadow/master/Scripts/DarkDexExplorer.lua"))()
	end)
end
coroutine.wrap(HIGFUKZ_fake_script)()
local function ETIY_fake_script() -- DriveWorld.LocalScript 
	local script = Instance.new('LocalScript', DriveWorld)

	DriveWorld.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/IFearF4ded/Uncopylock-Maps/main/Drive%20World%20Script'))()
	end)
end
coroutine.wrap(ETIY_fake_script)()
local function EHQTUJ_fake_script() -- Camlock.LocalScript 
	local script = Instance.new('LocalScript', Camlock)

	Camlock.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/IFearF4ded/Uncopylock-Maps/main/Lock'))()
	end)
end
coroutine.wrap(EHQTUJ_fake_script)()
